    "base": {
      "$type": "fontSizes",
      "$value": "1.333"
    },
    "root": {
      "$type": "fontSizes",
      "$value": "14"
    },
    "Small": {
      "$type": "fontSizes",
      "$value": "{Medium} / {base}"
    },
    "Medium": {
      "$type": "fontSizes",
      "$value": "{root}"
    },
    "Large": {
      "$type": "fontSizes",
      "$value": "{Medium} * {base}"
    },
    "XL": {
      "$type": "fontSizes",
      "$value": "{Large} * {base}"
    }
  },
  "light": {
