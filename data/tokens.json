{
  "global": {
    "baseSpace": {
      "$type": "spacing",
      "$value": "4"
    },
    "space-1": {
      "$type": "spacing",
      "$value": "{baseSpace}"
    },
    "space-2": {
      "$type": "spacing",
      "$value": "{baseSpace} *2"
    },
    "space-3": {
      "$type": "spacing",
      "$value": "{baseSpace} *3"
    },
    "space-4": {
      "$type": "spacing",
      "$value": "{baseSpace} *4"
    },
    "space-5": {
      "$type": "spacing",
      "$value": "{baseSpace} *5"
